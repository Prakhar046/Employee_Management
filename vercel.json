{
    "builds": [{
        "src": "Employee_Management/wsgi.py",
        "use": "@vercel/python3.9",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9", "maxDuration": 60 }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Employee_Management/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "static/$1"
        }
    ]
}